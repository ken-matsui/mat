import std.io;
import stdio;

let fuga: i32 = 1;

type newint = i32;
type User = newint;


fn f1(arg: char, arg2: u64) -> u32 {
    return (u64)arg + arg2;
}

fn main() -> i32 {
    let mut hoge: User = 12;
    if hoge {
        return 1;
    } else if fuga {
        return (i32)f1((char)fuga, (u64)hoge);
    } else {
        return 1 + 2 + 2 - 1i64 * hoge;
    }
}
